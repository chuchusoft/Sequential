/* The primary screen. */
"Main Screen" = "Tela Principal";

/* Non-primary screens. %lu is replaced with the screen number. */
"Screen %lu" = "Tela %lu";

/* Clear the Open Recent menu. Should be the same as the standard text. */
"Clear Menu" = "Limpar Menu";

/* Label for when no image is being displayed in the window. */
"No image" = "Sem imagem";

/* Update menu item title. One of two states. */
"Check for Updates..." = "Verificar Atualizações…";
"Install Update..." = "Instalar Atualização…";

/* Reveal in Finder, Path Finder (www.cocoatech.com) or web browser. Three states of the same item. */
"Reveal in Finder" = "Mostrar no Finder";
"Reveal in Path Finder" = "Mostrar no Path Finder";
"Reveal in Browser" = "Mostrar no Navegador";

/* Switch to Finder or Path Finder (www.cocoatech.com). Two states of the same item. */
"Switch to Finder" = "Alternar para Finder";
"Switch to Path Finder" = "Alternar para Path Finder";

/* Title of save dialog when setting a copy as the desktop picture. */
"Save Copy as Desktop Picture" = "Salvar Cópia como Imagem do Desktop";

/* Enter/exit full screen. Two states of the same item. */
"Enter Full Screen" = "Entrar Tela Inteira";
"Exit Full Screen" = "Sair Tela Inteira";

/* Lets the user toggle the on-screen display. Two states of the same item. */
"Show Info" = "Mostrar Info";
"Hide Info" = "Esconder Info";

/* Lets the user toggle whether thumbnails are shown. Two states of the same item. */
"Show Thumbnails" = "Mostrar Thumbnails";
"Hide Thumbnails" = "Esconder Thumbnails";

/* Title of menu item for toggling animation. Two states. */
"Turn Animation On" = "Ligar Animação";
"Turn Animation Off" = "Desligar Animação";

/* Scale image down so the entire thing fits menu item. Two labels, depending on mode. */
"Fit to Window" = "Ajustar a Janela";
"Fit to Screen" = "Ajustar a Tela";

/* The title of the bookmarks menu. Two states. */
"Resume" = "Continuar";
"Delete" = "Deletar";

/* Bookmark menu item used when the file named %@ cannot be found. */
"Missing File %@" = "Arquivo %@ não encontrado";

/* Display string for timer intervals. %@ is replaced with the remaining seconds and tenths of seconds.*/
"%@ seconds" = "%@ segundos";

/* Units (bytes, kilobytes, etc). */
"B" = "B";
"KB" = "KB";
"MB" = "MB";
"GB" = "GB";
"TB" = "TB";

/* Title of general pref pane. */
"General" = "Geral";

/* Title of navigation pref pane. */
"Navigation" = "Navegação";

/* Title of update pref pane. */
"Update" = "Atualização";

/* Offer to resume from bookmark alert message text. %@ is replaced with the page name. */
"This document has a bookmark for the page %@." = "Este documento tem a página %@ marcada como favorita.";

/* Offer to resume from bookmark alert informative text. */
"If you don't resume from this page, the bookmark will be kept and you will start from the first page as usual." = "Se você não continuar a partir desta página, a marcação como favorito será mantida e você terá de começar a partir da primeira página como de costume.";

/* Don't resume from bookmark button. */
"Don't Resume" = "Não Continuar";

/* Do resume from bookmark button. */
"Resume" = "Continuar";

/* The URL returned a error status code. %ld is replaced by the status code, the first %@ is replaced by the human-readable error (automatically localized), the second %@ is replaced by the full URL. */
"The error %ld %@ was generated while loading the URL %@." = "O erro “%ld %@” foi gerado ao carregar a URL %@.";

/* The URL could not be loaded for an unknown reason. %@ is replaced by the full URL. */
"The URL %@ could not be loaded." = "A URL %@ não pode ser carregada.";

/* Replace file alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"%@ already exists in %@. Do you want to replace it?" = "%@ já existe em %@. Deseja substituir?";

/* Replace multiple files alert. %lu is replaced with a number greater than 1, %@ is replaced with the destination name. */
"%lu pages already exist in %@. Do you want to replace them?" = "%lu páginas já existem em %@. Deseja substituir?";

/* Informative text for replacement alerts. */
"Replacing a file overwrites its current contents." = "Substituir um arquivo sobrescreve seu conteúdo atual.";

/* Single image save failure alert. The first %@ is replaced with the filename, the second is replaced with the destination name. */
"The image %@ could not be saved to %@." = "A imagem %@ não pode ser salva em %@.";

/* Multiple image save failure alert. %lu is replaced with the number of files, %@ is replaced with the destination name. */
"%lu images could not be saved to %@." = "%lu imagens não puderam ser salvas em %@.";

/* Informative text for save failure alerts. */
"Make sure the volume is writable and has enough free space." = "Certifique-se que o dispositivo tenha permissão de escrita e espaço livre suficiente.";

/* Appears in the image save alert beside each filename that conflicts with an existing file in the destination folder. */
"File already exists." = "Arquivo já existe.";

/* Bookmarked file could not be found error. %@ is replaced with the missing page's saved filename. */
"The file referenced by the bookmark %@ could not be found." = "O arquivo referenciado como favorito %@ não foi encontrado.";

/* Bookmarked file could not be found error informative text. */
"It may have been moved or deleted." = "Pode ter sido movido ou deletado.";

/* XADMaster error reporting. %@ is replaced with the XADMaster error. */
"The error %@ occurred while parsing the archive." = "Ocorreu o erro %@ durante a análise do arquivo.";

/* Informative string for secondary mouse button action. */
"Secondary click goes to the previous page." = "Clique secundário do mouse leva a página anterior.";
"Secondary click goes right." = "Clique secundário do mouse leva a direita.";
"Secondary click goes left." = "Clique secundário do mouse leva a esquerda.";

/* Flickr generic error message. %@ is replaced with the message as returned by Flickr. */
"Flickr returned the error %@." = "Flickr retornou o erro %@.";

/* Flickr user not found error message. %@ is replaced with the user name/NSID. */
"Flickr could not find the user %@. This user may not exist or may have disabled searches in the Flickr privacy settings." = "Flickr não encontrou o usuário %@. Esse usuário pode não existir ou ter desabilitado as pesquisas nas preferências de privacidade do Flickr.";

/* Dialog button labels. */
"OK" = "OK";
"Cancel" = "Cancelar";
"Delete Bookmark" = "Deletar Favorito";
"Replace" = "Substituir";
"Choose" = "Escolher";